        html, body {
            background-color: #f1f1f1;
            border: 0;
            margin: 0;
            padding: 0;
            width: 100%;
            font-size: 16px;
        }

        a {
            color: #2f9bbf;
            text-decoration: none;
        }
            [type="checkbox"]:not(:checked), [type="checkbox"]:checked {
        position: absolute;
        left: -9999px;
        opacity: 0
    }

        .infogeek-profile .profile {
            position: relative;
            text-transform: uppercase;
            width: 80%;
            margin-top: 8%;
            font-family: "Open Sans", sans-serif;
            margin-left: auto;
        }

        .infogeek-profile .picture-container {
            display: inline-block;
            position: absolute;
            top: 1em;
            left: 10%;
            width: 10em;
            height: 10em;
            border-width: 0;
            border-radius: 5em;
            overflow: hidden;
            z-index: 3;
            box-shadow: 1px 1px 2px 1px #bbb;
        }

            .infogeek-profile .picture-container img {
                width: 140%;
                position: relative;
                right: 30%;
            }

        .infogeek-profile .info {
            text-align: center;
            background-color: #fff;
            display: inline-block;
            position: relative;
            left: -2em;
            width: 70%;
            min-height: 20em;
            padding: 1em 2em;
            border: .5px solid #f0f0f0;
            border-radius: .3em;
            z-index: 1;
            box-shadow: 1px 1px 2px 1px #bbb;
        }

            .infogeek-profile .info hr {
                border: none;
                border-bottom: 1px solid #ddd;
                width: 113%;
                margin-left: -2em;
            }

        .infogeek-profile .info {
            color: #666;
        }

            .infogeek-profile .info header * {
                padding: .1em;
                font-weight: 100;
            }

            .infogeek-profile .info header h2 {
                display: block;
            }

            .infogeek-profile .info header h3 {
                font-size: 80%;
                font-weight: 500;
                color: #444;
            }

                .infogeek-profile .info header h3 .label:not(:first-child) {
                    padding-left: 1em;
                }

            .infogeek-profile .info .info-container {
                display: table;
                margin-top: 1em;
            }

                .infogeek-profile .info .info-container p {
                    display: table-row;
                }

                    .infogeek-profile .info .info-container p .label,
                    .infogeek-profile .info .info-container p .value {
                        display: table-cell;
                        color: #777;
                        height: 2em;
                    }

            .infogeek-profile .info .label:after {
                content: " : ";
            }

            .infogeek-profile .info .info-container p .value {
                text-transform: none;
                color: #444;
                padding-left: 5em;
            }
    </style>


    <div class="infogeek-profile">
         <section class="profile">

        <div class="photo">
            <div>
                <article class="info" style="float: left;">
                    <div class="picture-container">
                        @{
                            if (@ViewBag.Photo != null)
                            {
                                var base64 = Convert.ToBase64String(@ViewBag.Photo);
                                var imagesrc = string.Format("data:image/jpeg;base64,{0}", base64);

                                <img src="@imagesrc" alt="" class="circle responsive-img activator card-profile-image" />
                            }
                            else
                            {


                                <img src="~/Resource/thuan-pham-1Kj0OZiwLN8-unsplash.jpg" alt="" class="circle responsive-img activator card-profile-image" />

                            }
                        }

                    </div>
                    <div class="share"></div>
                    <header>
                        <h2>@ViewBag.UserName</h2>
                        <h3>
                            @Resources.Content.Email
                            <span class="label"> </span>@ViewBag.Email
                        </h3>
                    </header>
                    <hr />
                    <div class="info-container">
                        <p>
                            <span class="label date-of-birth">@Resources.Content.Status</span>
                            <span class="value date-of-birth">@ViewBag.Status</span>
                        </p>
                        <p>
                            <span class="label date-of-birth">@Resources.Content.Phone</span>
                            <span class="value date-of-birth">@ViewBag.PhoneNumber</span>
                        </p>

                        <p>
                            <span class="label date-of-birth">@Resources.Content.Experience</span>
                            <span class="value date-of-birth"> @ViewBag.Experience</span>
                        </p>
                        <p>
                            <span class="label date-of-birth">@Resources.Content.Position</span>
                            <span class="value date-of-birth"> @ViewBag.Position</span>
                        </p>
                        <p>
                            <span class="label date-of-birth">@Resources.Content.WorkPlace</span>
                            <span class="value date-of-birth"><a href="#">@ViewBag.WorkPlace</a></span>
                        </p>
                        <p>
                            <span class="label date-of-birth">@Resources.Content.AdditionalInfo</span>
                            <span class="value date-of-birth"> @ViewBag.Info</span>
                        </p>
                    </div>
                </article>
            </div>
            <div style="float: left;">
                <div class="container" style="margin: 0 auto;
    max-width: 20em;
    margin-left: inherit;">
                    <p>@ViewBag.Error</p>
                    @if (@ViewBag.Following == true)
                    {<a href="@Url.Action("Unfollow", "Account",new { FollowingId=@ViewBag.Id})" class="elements">
                            <button type="button" class="btn  btn-block " style="background-color:#adabaa; color:aliceblue">

                                <span>@Resources.Content.Unfollow</span>

                            </button>
                        </a>
                    }
                    else
                    { <a href="@Url.Action("AddUser", "Account",new { FollowingId=@ViewBag.Id})" class="elements">
                            <button type="button" class="btn  btn-block " style="background-color:#4da323; color:aliceblue">

                                <span>@Resources.Content.Follow</span>

                            </button>
                        </a>
                    }
                    <a href="@Url.Action("DesignAnotherUser", "Design", new { id=@ViewBag.Id})" class="elements">
                        <button type="button" class="btn  btn-block ">

                            <span>@Resources.Content.Gallary</span>

                        </button>
                    </a>

                </div>
            </div>
        </div>
    </section>
